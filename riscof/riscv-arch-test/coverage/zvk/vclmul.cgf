vclmul.vv:
    config:
      - check ISA:=(.*I.*V.*Zvbc)
    mnemonics:
      vclmul.vv: 0
    rs1:
      <<: *all_vregs
    rs2:
      <<: *all_vregs
    rd:
      <<: *all_vregs
    op_comb:
      <<: *sfmt_op_comb

vclmul.vx:
    config:
      - check ISA:=(.*I.*V.*Zvbc)
    mnemonics:
      vclmul.vx: 0
    rs1:
      <<: *all_regs
    rs2:
      <<: *all_vregs
    rd:
      <<: *all_vregs
    op_comb:
      <<: *sfmt_op_comb
